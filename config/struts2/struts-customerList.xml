<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>
	<package name="customerList" extends="common" namespace="/customerList">
		<action name="customerList_*" class="customerListAction" method="{1}">
			<result name="show">/com/snacks/customerList/jsp/customerList.jsp</result>
			<result name="getList" type="json"></result>
			<result name="operFandianUser" type="json"></result>
			<result name="getCodeArea" type="json"></result>
			<result name="setFandianId" type="json"></result>
			<result name="detail">/com/snacks/customerList/jsp/customerList_detail.jsp</result>
			<result name="getCaipinByFandianId" type="json"></result>
		</action>
	</package>
</struts>

